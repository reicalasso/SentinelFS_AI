{
  "dashboard": {
    "id": null,
    "title": "SentinelFS AI - Production Monitoring",
    "tags": ["sentinelfs", "ai", "monitoring", "production"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "API Request Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(sentinelfs_requests_total[5m])",
            "legendFormat": "{{method}} {{endpoint}}",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Request Latency",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(sentinelfs_request_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, rate(sentinelfs_request_duration_seconds_bucket[5m]))",
            "legendFormat": "50th percentile",
            "refId": "B"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Inference Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(sentinelfs_inference_duration_seconds_bucket[5m])) * 1000",
            "legendFormat": "95th percentile (ms)",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, rate(sentinelfs_inference_duration_seconds_bucket[5m])) * 1000",
            "legendFormat": "50th percentile (ms)",
            "refId": "B"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "Prediction Results",
        "type": "bargauge",
        "targets": [
          {
            "expr": "rate(sentinelfs_predictions_total[5m])",
            "legendFormat": "{{result}}",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        }
      },
      {
        "id": 5,
        "title": "Model Drift Score",
        "type": "graph",
        "targets": [
          {
            "expr": "sentinelfs_model_drift_score",
            "legendFormat": "Drift Score",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 6,
        "title": "System Resources",
        "type": "graph",
        "targets": [
          {
            "expr": "sentinelfs_memory_usage_bytes / 1024 / 1024",
            "legendFormat": "Memory Usage (MB)",
            "refId": "A"
          },
          {
            "expr": "sentinelfs_gpu_memory_usage_bytes / 1024 / 1024",
            "legendFormat": "GPU Memory (MB)",
            "refId": "B"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        }
      },
      {
        "id": 7,
        "title": "Active Connections",
        "type": "graph",
        "targets": [
          {
            "expr": "sentinelfs_active_connections",
            "legendFormat": "Active Connections",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 24
        }
      },
      {
        "id": 8,
        "title": "Alert Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(sentinelfs_alerts_total[5m])",
            "legendFormat": "{{type}} {{severity}}",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 24
        }
      },
      {
        "id": 9,
        "title": "Model Accuracy",
        "type": "singlestat",
        "targets": [
          {
            "expr": "sentinelfs_model_accuracy",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 32
        }
      },
      {
        "id": 10,
        "title": "Error Rate",
        "type": "singlestat",
        "targets": [
          {
            "expr": "rate(sentinelfs_requests_total{status=~\"5..\"}[5m]) / rate(sentinelfs_requests_total[5m]) * 100",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 32
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {},
    "templating": {
      "list": []
    },
    "annotations": {
      "list": []
    },
    "refresh": "30s",
    "schemaVersion": 27,
    "version": 0,
    "links": []
  }
}