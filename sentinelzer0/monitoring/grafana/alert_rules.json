              "datasourceUid": "prometheus",
              "model": {
                "editorMode": "builder",
                "expr": "histogram_quantile(0.95, rate(sentinelfs_request_duration_seconds_bucket[5m])) > 2",
                "legendFormat": "__auto",
                "range": true,
                "refId": "A"
              }
            },
            {
              "refId": "B",
